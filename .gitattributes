# Git LFS tracking for large files
*.zip filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.pt filter=lfs diff=lfs merge=lfs -text
*.pth filter=lfs diff=lfs merge=lfs -text
*.safetensors filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text
*.model filter=lfs diff=lfs merge=lfs -text
models/**/* filter=lfs diff=lfs merge=lfs -text
data/oracc/epsd2-literary/epsd2/literary/index-lem.json filter=lfs diff=lfs merge=lfs -text
data/oracc/epsd2-royal/epsd2/royal/gloss-sux.json filter=lfs diff=lfs merge=lfs -text
data/oracc/epsd2-literary/epsd2/literary/gloss-sux.json filter=lfs diff=lfs merge=lfs -text
